{
  "version": 2,
  "name": "orthodox-document-search",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/mro/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/gemini",
      "dest": "/api/gemini-proxy.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini-api-key",
    "GOOGLE_API_KEY": "@google-api-key"
  }
}
